    <system:String x:Key="menu.tools.project.expressions">表情...</system:String>
    <system:String x:Key="menu.tools.prefs">使用偏好...</system:String>

    <system:String x:Key="menu.help">帮助</system:String>
    <system:String x:Key="menu.help.about">关于 OpenUTAU</system:String>

    <!-- Preferences -->
    <system:String x:Key="prefs.caption">使用偏好</system:String>
    <system:String x:Key="prefs.paths">路径</system:String>
    <system:String x:Key="prefs.paths.singer">歌手路径</system:String>
    <system:String x:Key="prefs.paths.add">添加</system:String>
    <system:String x:Key="prefs.paths.remove">移除</system:String>
    <system:String x:Key="prefs.playback">回放</system:String>
    <system:String x:Key="prefs.playback.device">回放设备</system:String>
    <system:String x:Key="prefs.playback.test">测试</system:String>
    <system:String x:Key="prefs.rendering">渲染</system:String>
    <system:String x:Key="prefs.rendering.resampler.preview">预览用重采样器</system:String>
    <system:String x:Key="prefs.rendering.resampler.export">导出用重采样器</system:String>
    <system:String x:Key="prefs.rendering.resampler.note">
        注意: 将你选择的重采样器 exe 或者 dll 文件放入 Resamplers 文件夹中并在使用偏好中选择它.
    </system:String>
    <system:String x:Key="prefs.appearance">外观</system:String>
    <system:String x:Key="prefs.appearance.theme">主题</system:String>
    <system:String x:Key="prefs.appearance.theme.light">亮</system:String>
    <system:String x:Key="prefs.appearance.theme.dark">暗</system:String>
    <system:String x:Key="prefs.appearance.theme.note">注意: 切换主题后请重启OpenUtau</system:String>

    <!-- Dialogs -->
    <system:String x:Key="dialogs.noresampler.caption">无重采样器</system:String>
    <system:String x:Key="dialogs.noresampler.message">无重采样器! 将你选择的重采样器 exe 或者 dll 文件放入 Resamplers 文件夹中并在使用偏好中选择它!</system:String>
    <system:String x:Key="dialogs.about.caption">关于 OpenUTAU</system:String>
    <system:String x:Key="dialogs.about.message">
        OpenUtau 是一个开源编辑环境, 旨在为 UTAU 用户社区提供现代的用户体验和智能的语音学支持. 请到 Github 访问我们.
    </system:String>
    <system:String x:Key="dialogs.export.caption">导出</system:String>
    <system:String x:Key="dialogs.export.savefirst">请先保存工程文件</system:String>

    <!-- Tracks -->
    <system:String x:Key="tracks.remove">移除</system:String>
    <system:String x:Key="tracks.moveup">上移</system:String>
    <system:String x:Key="tracks.movedown">下移</system:String>

    <!-- Errors -->
    <system:String x:Key="errors.caption">错误</system:String>
    <system:String x:Key="warning">警告</system:String>
    <system:String x:Key="warning.asciipath">OpenUtau路径"{0}"包含非ASCII字符. Exe格式resampler可能无法正常工作.</system:String>

    <!-- Tips -->
    <system:String x:Key="tip.lyricbox.prev">Shift+Tab: 上一个</system:String>
    <system:String x:Key="tip.lyricbox.next">Tab: 下一个</system:String>
    <system:String x:Key="tip.notes.basics.keys" xml:space="preserve">T:
鼠标左键:
鼠标右键:
按住鼠标左键:
拖动音符:
拖动音符尾部:
Alt + 拖动音符尾部:
Ctrl + 框选:
上方向键/下方向键:
Ctrl + 上方向键/下方向键:
空格:</system:String>
    <system:String x:Key="tip.notes.basics.actions" xml:space="preserve">开/关提示
添加音符
删除音符
删除多个音符
移动音符
调整音符长度
调整相邻两个音符长度
选择音符
为选中音符移调
为选中音符移调八度
播放</system:String>
    <system:String x:Key="tip.notes.zoomx">◀ 鼠标滚轮横向缩放 ▶</system:String>
    <system:String x:Key="tip.notes.zoomy">鼠标滚轮纵向缩放 ▶</system:String>
    <system:String x:Key="tip.exps.keys" xml:space="preserve">按住鼠标左键:
按住鼠标右键:</system:String>
    <system:String x:Key="tip.exps.actions" xml:space="preserve">设定表情
重置表情</system:String>

</ResourceDictionary>